{
  "name": "sutando",
  "version": "1.6.6",
  "packageManager": "pnpm@7.33.7",
  "description": "A modern Node.js ORM. Makes it enjoyable to interact with your database. Support Mysql, MSSql, MariaDB, Sqlite.",
  "homepage": "https://sutando.org/",
  "bin": {
    "sutando": "./bin/cli.js"
  },
  "main": "sutando",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": {
          "types": "./types/index.d.ts",
          "default": "./sutando.mjs"
        },
        "require": {
          "types": "./types/index.d.ts",
          "default": "./sutando.js"
        }
      },
      "browser": {
        "import": {
          "types": "./types/browser/index.d.ts",
          "default": "./src/browser/sutando.mjs"
        },
        "require": {
          "types": "./types/browser/index.d.ts",
          "default": "./src/browser/sutando.js"
        }
      },
      "default": {
        "import": {
          "types": "./types/index.d.ts",
          "default": "./sutando.mjs"
        },
        "require": {
          "types": "./types/index.d.ts",
          "default": "./sutando.js"
        }
      }
    },
    "./browser": {
      "import": {
        "types": "./types/browser/index.d.ts",
        "default": "./src/browser/sutando.mjs"
      },
      "require": {
        "types": "./types/browser/index.d.ts",
        "default": "./src/browser/sutando.js"
      }
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sutandojs/sutando.git"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=4"
  },
  "scripts": {
    "test:mysql": "cross-env DB=mysql jest --selectProjects node",
    "test:postgres": "cross-env DB=postgres jest --selectProjects node",
    "test:sqlite": "cross-env DB=sqlite jest --selectProjects node",
    "test:browser": "jest --selectProjects browser"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "collect.js": "^4.34.3",
    "colorette": "^2.0.20",
    "commander": "^11.1.0",
    "dayjs": "^1.11.5",
    "escalade": "^3.1.1",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.0",
    "mysql2": "3.12.0",
    "pg": "8.8.0",
    "prettier": "2.6.2",
    "sqlite3": "5.1.7",
    "tedious": "15.1.0"
  },
  "keywords": [
    "sutando",
    "ORM",
    "mysql",
    "mariadb",
    "sqlite",
    "postgresql",
    "postgres",
    "pg",
    "mssql",
    "active record"
  ],
  "author": "Kidd Yu",
  "readmeFilename": "README.md",
  "license": "MIT"
}
